            <script type="text/javascript">
                var handleTabs = function (containerId) {

                    document.querySelectorAll('#' + containerId + ' .tab').forEach(function (elem) {
                        elem.setAttribute('style', 'display: none;')
                    });

                    document.querySelectorAll('#' + containerId + ' ul.menu li a').forEach(function (el) {
                        el.addEventListener('click', function (e) {
                            e.preventDefault();
                            document.querySelectorAll('#' + containerId + ' ul.menu li').forEach(function (elem) {
                                elem.classList.remove('selected');
                            });
                            document.querySelectorAll('#' + containerId + ' .tab').forEach(function (elem) {
                                elem.setAttribute('style', 'display: none;')
                            });
                            document.querySelectorAll(el.getAttribute('href')).forEach(function (elem) {
                                elem.setAttribute('style', 'display: block;')
                            });
                            e.currentTarget.parentNode.classList.add('selected');
                            if (typeof e.currentTarget.getAttribute('type') !== 'undefined') {
                                var type = e.currentTarget.getAttribute('type');
                                var type_saved = e.currentTarget.value;
                                if (type === 'radio' && type_saved === 'sradio') {
                                    type = type_saved;
                                }
                                document.getElementById('type').value = type;
                                dzmusicplugin.pluginUpdateParam('player', e.currentTarget);
                            }
                        });
                    });

                };

                var updateColorpicker = function (hex) {
                    var target = document.getElementById('plugin_color');
                    target.setAttribute('style', 'color:' + hex + ';');
                    target.value = hex;

                    dzmusicplugin.pluginUpdateParam('player', target);
                    return true;
                };

                var initPage = function () {

                    // First we check if the externalizedScript var in window global object is here.
                    // It will define if the below code is in slash or not, to avoid breaks in production
                    if(typeof window.externalizedScript !== 'undefined'){
                        return false;
                    }

                    document.querySelectorAll('#searchboxalbum input.search_input').forEach(function(elem) {
                        elem.addEventListener('keyup', function (e) {
                            return searchtool.onKeyUp(e, 'album');
                        });
                    });

                    document.querySelectorAll('#searchboxsmart input.search_input').forEach(function (elem) {
                        elem.addEventListener('keyup', function (e) {
                            return searchtool.onKeyUp(e, 'artist');
                        });
                    });

                    document.querySelectorAll('#widget-options input[type=text], ul.generator input[type=text]').forEach(function (elem) {
                        elem.addEventListener('keyup', function (e) {
                            if (e.keyCode === 13) {
                                /* Enter key */
                                dzmusicplugin.pluginUpdateParam('player', e);
                            }
                        });
                    });

                    document.querySelectorAll('#widget-options select').forEach(function (elem) {
                        elem.addEventListener('change', function(e){
                            document.getElementById('id_application').value = '';
                            if(e.currentTarget.value !== ''){
                                document.getElementById('id_application').value = '';
                                dzmusicplugin.pluginUpdateParam('player', e.currentTarget);
                            }
                        });
                    });

                    document.querySelectorAll('.js-selectchange-update-widget').forEach(function (elem) {
                        elem.addEventListener('change', function (e) {
                            if(e.currentTarget.value !== ''){
                                e.currentTarget.closest('.tab').querySelector('.js-selectchange-data-target').value = e.currentTarget.value;
                                dzmusicplugin.pluginUpdateParam('player', e.currentTarget);
                            }
                        });
                    });

                    document.querySelectorAll('.form-select').forEach(function (elem) {
                        elem.addEventListener('change', function (e) {
                            dzmusicplugin.pluginUpdateParam('player', e.currentTarget);
                        })
                    });

                    document.querySelectorAll('.js-blur-update-widget').forEach(function (elem) {
                        elem.addEventListener('blur', function (e) {
                            dzmusicplugin.pluginUpdateParam('player', e.currentTarget);
                        });
                    });

                    document.querySelectorAll('.js-click-update-widget').forEach(function (elem) {
                        elem.addEventListener('click', function (e) {
                            // Prevent default only if not radio button
                            if(e.currentTarget.type !== 'radio'){
                                e.preventDefault();
                            }
                            dzmusicplugin.pluginUpdateParam('player', e.currentTarget);
                        });
                    });

                    handleTabs('widget-tabs');
                    handleTabs('widget-generated-code');


                    document.getElementById('tabs-code-iframe').setAttribute('style', 'display: block;');
                    var defaultTab = document.getElementById('tabs-playlist');
                    defaultTab.setAttribute('style', 'display: block;');
                    dzmusicplugin.pluginUpdateParam('player', defaultTab);

                    var picker = new CP(document.querySelector('#colorpicker .btn'));
                    picker.on("change", function (color) {
                        updateColorpicker('#' + color);
                    });
                    document.addEventListener('click', function (e) {
                        if (!e.target.matches('.color-picker, .color-picker *, #colorpicker .btn, #colorpicker .btn *')) {
                            e.stopPropagation();
                            picker.exit();
                        }
                    });

                    var default_ids = document.getElementById('content_api').getAttribute('data-default-ids');
                };

                if (document.readyState === 'complete' || (document.readyState !== 'loading' && !document.documentElement.doScroll)) {
                    initPage();
                } else {
                    document.addEventListener('DOMContentLoaded',initPage);
                }
            </script>
