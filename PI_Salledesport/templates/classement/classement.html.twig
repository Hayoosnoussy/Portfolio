        <div class="row">
            <!div class="ts-slider owl-carousel">
            <!------ recherche num abonné--------------------------------------->
            {% set tabs=[] %}
            {% for coach in coach %}
                {%  set numabb = 0 %}
                {% for abbs in abbs %}
                    {% if abbs.coach.id == coach.id %}
                        {%  set numabb = numabb + 1 %}
                    {% endif%}
                {% endfor %}
                {% set tabs = tabs|merge(
                    [{coach: coach.user.nom , abbo: numabb }],
                    )%}
            {% endfor%}
            <!----classement ------------------------>
            {% set classement = 1 %}
            {% for tab in tabs|sort((b,a)=>a.abbo <=> b.abbo) %}
                {% for coach in coach  %}
                    {% if coach.user.nom == tab.coach %}
                        <!--------affichage----------------------->
                        <div class="col-lg-4">
                            <h4  style="color: white; background-color: olive">{{ classement }} : {{ tab.coach }}</h4>
                            <div class="ts-item set-bg" data-setbg="{{ asset('uploads/images/' ~ coach.image) }}">
                                <div class="ts_text">
                                    <h4> {{ tab.coach }}  </h4>
                                    <span>{{ coach.speciality }}</span>
                                    <h2 style="color: orangered">nombre d'abonnées : {{ tab.abbo }}</h2>
                                </div>
                            </div>
                        </div>
                        {% set classement = classement+1 %}
                    {% endif %}

                {% endfor %}
            {% endfor %}

        </div>
    </div>
