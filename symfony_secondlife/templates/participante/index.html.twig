{% extends 'base.html.twig' %}

{% block title %}Participante index{% endblock %}

{% block body %}
    <h1>Participante index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>IdParticipant</th>
                <th>Comptelogin</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for participante in participantes %}
            <tr>
                <td>{{ participante.idParticipant }}</td>
                <td>{{ participante.comptelogin }}</td>
                <td>
                    <a href="{{ path('participante_show', {'idParticipant': participante.idParticipant}) }}">show</a>
                    <a href="{{ path('participante_edit', {'idParticipant': participante.idParticipant}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('participante_new') }}">Create new</a>
{% endblock %}
