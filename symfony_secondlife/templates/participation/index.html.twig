{% extends 'BaseFront.html.twig' %}

{% block title %}Participation index{% endblock %}

{% block body %}


    <table class="table">
        <thead>
            <tr>
                <th>Idseance</th>
                <th>Idroutine</th>
                <th>Idclient</th>
                <th>Idcoach</th>
                <th>Status</th>
                <th>Idroutine</th>
            </tr>
        </thead>
        <tbody>
        {% for participation in participations %}
            <tr>
                <td>{{ participation.idseance }}</td>
                <td>{{ participation.idclient }}</td>
                <td>{{ participation.idcoach }}</td>
                <td>{{ participation.status }}</td>
                <td>{{ participation.idroutine }}</td>

            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('participation_new ', {'idseance': seance.idseance}) }}">Participer</a>
{% endblock %}
